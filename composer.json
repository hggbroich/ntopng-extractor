{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3.10.4",
        "doctrine/doctrine-bundle": "^2.18.2",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.2",
        "dragonmantank/cron-expression": "^3.6",
        "gpslab/geoip2": "^2.1.3",
        "knplabs/knp-menu-bundle": "^3.7",
        "knplabs/knp-time-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3.2",
        "ramazancetinkaya/byte-formatter": "^1.0",
        "runtime/frankenphp-symfony": "^1.0",
        "schulit/common-bundle": "^6.0.2",
        "symfony/console": "^7.4.4",
        "symfony/doctrine-messenger": "^7.4.5",
        "symfony/dotenv": "^7.4",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "^7.4.5",
        "symfony/http-client": "^7.4.5",
        "symfony/messenger": "^7.4.4",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/property-access": "^7.4.4",
        "symfony/property-info": "^7.4.5",
        "symfony/runtime": "^7.4.1",
        "symfony/scheduler": "^7.4.4",
        "symfony/serializer": "^7.4.5",
        "symfony/twig-bundle": "^7.4.4",
        "symfony/web-link": "^7.4.4",
        "symfony/webpack-encore-bundle": "^2.4",
        "symfony/yaml": "^7.4.1",
        "twig/extra-bundle": "^2.12|^3.23",
        "twig/twig": "^2.12|^3.23"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "geoip2:update": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4"
        }
    }
}
